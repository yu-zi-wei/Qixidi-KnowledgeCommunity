<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qixidi.business.mapper.tool.MysqlToolMapper">
    <update id="emptyData">
        truncate table ${tableName}
    </update>

    <update id="emptyDataList">
        <foreach item="tableName" index="index" collection="yesList">
            truncate table ${tableName};
        </foreach>
    </update>
</mapper>
